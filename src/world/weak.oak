(define-constant object-hash
  (add-method ((make-open-coded-operation '((object-hash)) 1 1)
	       (object) x)
    (object-hash x)))

(define-constant object-unhash
  (add-method ((make-open-coded-operation '((object-unhash)) 1 1)
	       (fixnum) x)
    (object-unhash x)))

